{
    "manifest_version": 2,

    "name": "金数据新数据提醒",
    "description": "当您的表单有新数据提交时，通过chrome提醒您。",
    "version": "1.0",

    "icons": {
        "128": "images/icon38.png"
    },

    "permissions": [
        "https://jinshuju.net/",
        "https://www.jinshuju.net/",
        "http://localhost:3000/",
        "storage",
        "notifications"
    ],
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        }
    },

    "background": {
        "scripts": ["backgroundPage.js"]
    },
    "web_accessible_resources": [
        "images/new_entry.png"
    ]
}